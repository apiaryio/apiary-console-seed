{
  "manifest_version": 2,
  "key": "",
  "name": "Apiary Proxy Chrome Extension",
  "version": "0.0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:*/*",
      "https://localhost:*/*"
    ]
  },
  "background": {
    "scripts": [
      "build/extension.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
